! Comment line
! stream input2
parameters file ../test/ec2acc_prep/toppar/par_all36_prot.prm
parameters file ../test/ec2acc_prep/toppar/par_all36_cgenff.prm 
parameters file ../test/toppar_water_ions/par_water_ions.prm
! parameters print
structure file psf ../test/ec2acc_prep/minimized.psf
structure print
selection print
selection define Water resname TIP3
selection define Backbone and atomnames N C NH O CA HA segid PROT
selection define WaterH and hydrogen selection Water
selection print
selection delete WaterH
selection print
msld nblocks 2
selection reset
selection define site1sub2 and and segid PROT resid 29 atomnames NK_A HK_A CK_A HK_B CK_C HK_C HK_D CK_D HK_E HK_F CK_E HK_G HK_H CK_F HK_I HK_J NK_B HK_K HK_L HK_M CK_B OK_A
selection define site1sub1 and and segid PROT resid 29 not selection site1sub2
msld call 1 site1sub1
msld call 2 site1sub2
!               B S   t0  tv  tm  fb   q
msld initialize 1 1  1.5 0.0 5.0 0.0 1.0
msld initialize 2 1 -1.5 0.0 5.0 0.0 1.0
msld removescaling bond angle ! scale urey dihe impr and cmap
!        s1s2 t  l0   k n
msld bias 1 2 6 0.0 0.0 0
structure setup
state file ../test/ec2acc_prep/minimized.pdb
state box 75.681191 0 0   0 75.681191 0   0 0 75.681191
state help
state help file
run setvariable nsteps 100
run setvariable dt 0.002
run setvariable T 300
run setvariable gamma 1
run setvariable fnmxtc test.xtc
run setvariable fnmlmd test.lmd
run setvariable freqxtc 10
run print
run dynamics
